<% 
%w(jquery-1.4.1.min jquery-ui-1.8.13.custom.min application jquery.jcarousel.pack jquery-func jquery.tablesorter gears_init geo yqlgeo qTip tipsy).each do |x| %>
	<%= javascript_include_tag "#{x}" %>
<% end %>

<script>
$(function() {
var tags = [];
<% Restaurant.all.each do |restaurant| %> 
tags.push("<%= restaurant.name %>, <%= restaurant.formatted_address %>");
<% end %>

$( 'input[title="Search"]' ).autocomplete({
source: tags
});
});
</script>